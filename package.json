{
  "name": "shards-react",
  "version": "1.0.3",
  "description": "A free, beautiful and modern React UI kit powered by Bootstrap 4 and the Shards UI kit.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "contributors": [
    {
      "name": "Catalin Vasile",
      "url": "https://github.com/hisk"
    }
  ],
  "scripts": {
    "build": "tsc",
    "start": "nodemon --port 3001",
    "lint": "tslint --format stylish --project .",
    "prettier": "prettier --config node_modules/tslint-lt/.prettierrc.yml --write \"**/*.{ts,tsx,css,scss,html,js,jsx,yml,json,md}\" \"!{,dist}/**\"",
    "test": "npm run prettier && npm run build && npm run lint",
    "prepare": "npm run build"
  },
  "author": "DesignRevision <contact@designrevision.com>",
  "license": "MIT",
  "keywords": [
    "shards",
